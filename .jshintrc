{
    "maxerr": 50, // máximo número de errores analizados antes de parar

    // Enforcing
    "bitwise": true, // true: Prohíbe las operaciones lógicas de bit (&, |, ^, etc.)
    "camelcase": false, // true: Obliga a los nombres de variables con camelCase
    "curly": true, // true: Prohíbe los bloques de sentencia única sin {}
    "eqeqeq": true, // true: Obliga a las comparaciones con triple igual (===)
    "forin": true, // true: Obliga al filtrado de bucles for..in loops
    "freeze": true, // true: Prohíble sobreescribir prototipos de objetos nativos
    "immed": false, // true: Obliga al uso de comillas en la invocación inmediata de funciones de este modo: `(function () { } ());`
    "indent": 4, // Número de espacios usados para la sangría de línea
    "latedef": false, // true: Obliga a la declaración de funciones antes de su uso
    "newcap": false, // true: Obliga al uso de mayúscula en las funciones constructor
    "noarg": true, // true: Prohíbe el uso de `arguments.caller` y `arguments.callee`
    "noempty": true, // true: Prohíbe el uso de bloques vacíos
    "nonbsp": true, // true: Prohibe caracteres en blanco inservibles
    "nonew": false, // true: Prohibe el uso de "new" sin asignación
    "plusplus": false, // true: Prohibe el uso de `++` y `--`
    "quotmark": false, // Consistencia de comillas:
    // false : no hace nada (por defecto)
    // true : fuerza aperturas con cierres
    // "single" : fuerza comillas simples
    // "double" : fuerza comillas dobles
    "undef": true, // true: Fuerza la declaración de variables globales
    "unused": true, // Detecta variables sin usar:
    // true : todas, con la última función
    // "vars" : todas
    // "strict" : todas y todas las funciones
    "strict": true, // true: Fuerza todas las funciones en modo estricto
    "maxparams": false, // {int} Número máximo de parámetros por función
    "maxdepth": false, // {int} Número máximo de funciones anidadas
    "maxstatements": false, // {int} Número máximo de sentencias por función
    "maxcomplexity": false, // {int} Número máximo de ciclos por función
    "maxlen": false, // {int} Número máximo de caracteres por línea

    // Relaxing
    "asi": false, // true: Permite inserción automática con Semicolon
    "boss": false, // true: Permite asignaciones con comparaciones
    "debug": false, // true: Permite sentencias de depuración
    "eqnull": false, // true: Permite el uso '== null'
    "es5": false, // true: Permite sintaxis ES5
    "esnext": false, // true: Permite sintaxis ES.next
    "moz": false, // true: Permite sintaxis específica de Mozilla
    "evil": false, // true: Permite el uso de 'eval' y 'new Function()'
    "expr": false, // true: Permite 'ExpressionStatement'
    "funcscope": false, // true: Permite definir variables en sentencias de control
    "globalstrict": false, // true: Permite el uso de "use strict" global
    "iterator": false, // true: Permite el uso de '__iterator__'
    "lastsemic": false, // true: Permite omitir ';' en bloques de una línea
    "laxbreak": false, // true: Permite saltos de línea inseguros
    "laxcomma": false, // true: Permite estilo 'comma-first'
    "loopfunc": false, // true: Permite definir funciones en bucles
    "multistr": false, // true: Permite cadenas multi línea
    "noyield": false, // true: Permite funciones generador vacías
    "notypeof": false, // true: Permite operadores 'typeof' inválidos
    "proto": false, // true: Permite el uso de '__proto__'
    "scripturl": false, // true: Permite script-targeted URLs
    "shadow": false, // true: Allows redefinición de variables
    "sub": false, // true: Permite notación '[]'
    "supernew": false, // true: Permite 'new function ()' y 'new Object;'
    "validthis": false, // true: Permite el uso de this en funciones no constructoras

    // Environments
    "browser": true, // Web Browser (window, document, etc)
    "browserify": false, // Browserify (node.js code in the browser)
    "couch": false, // CouchDB
    "devel": true, // Development/debugging (alert, confirm, etc)
    "dojo": false, // Dojo Toolkit
    "jasmine": false, // Jasmine
    "jquery": false, // jQuery
    "mocha": true, // Mocha
    "mootools": false, // MooTools
    "node": false, // Node.js
    "nonstandard": false, // Widely adopted globals (escape, unescape, etc)
    "phantom": false, // PhantomJS
    "prototypejs": false, // Prototype and Scriptaculous
    "qunit": false, // QUnit
    "rhino": false, // Rhino
    "shelljs": false, // ShellJS
    "typed": false, // Globals for typed array constructions
    "worker": false, // Web Workers
    "wsh": false, // Windows Scripting Host
    "yui": false, // Yahoo User Interface

    // Custom Globals
    "globals": {} // variables globales adicionales
}
